#------------------------------------------------------------------------------
# balance size of windows
# shift + alt - 0 : yabai -m space --balance
#
# make floating window fill screen
shift + cmd - up     : yabai -m window --grid 1:1:0:0:1:1
ctrl + cmd + shift + alt - up     : yabai -m window --grid 1:1:0:0:1:1
# center
shift + cmd - down   : yabai -m window --grid 8:8:1:1:6:6
ctrl + cmd + shift + alt - down     : yabai -m window --grid 8:8:1:1:6:6

# make floating window fill left-half of screen
shift + cmd - left   : yabai -m window --grid 1:2:0:0:1:1
ctrl + cmd + shift + alt - left   : yabai -m window --grid 1:2:0:0:1:1
# make floating window fill right-half of screen
shift + cmd - right  : yabai -m window --grid 1:2:1:0:1:1
ctrl + cmd + shift + alt - right  : yabai -m window --grid 1:2:1:0:1:1

